{
  "name": "Filter out non-original content",
  "description": "Remove retweets and shares from your social media feed",
  "version": "0.1",
  "permissions": [
    "storage", "tabs", "https://www.facebook.com/", "https://twitter.com/"
  ],
  "browser_action": {
      "default_title": "Original Content Only",
      "default_icon": "ocOnlyIcon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
